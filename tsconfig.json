{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "noImplicitReturns":true,
    "noUnusedLocals":true,
    "noUnusedParameters": true,
    "baseUrl": ".",
      "paths": {
          "@/components/*": ["./components/*"],
          "@/utils/*": ["./utils/*"],
          "@/screens/*": ["./screens/*"]
      },
  }
}
